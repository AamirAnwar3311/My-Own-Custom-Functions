{
    "name": "helperfunctions/helperfunctions",
    "type": "library",
    "description": "A comprehensive collection of helper functions for PHP/Laravel applications including API calls, calculations, date operations, and more.",
    "keywords": [
        "helper",
        "functions",
        "utilities",
        "laravel",
        "php",
        "api",
        "helpers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "HelperFunctions Contributors",
            "homepage": "https://github.com/AamirAnwar3311/My-Own-Custom-Functions"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.0|^3.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "HelperFunctions\\Helpers\\": "src/Helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HelperFunctions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HelperFunctions\\Helpers\\HelperFunctionsServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}